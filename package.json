{
  "name": "expense_app",
  "version": "1.0.0",
  "description": "A app that lists expenses and allows upload of receipts. Pleo frontend challenge",
  "main": "index.js",
  "author": "Arpit Bharti<arpitbharti73@gmail.com>",
  "repository": "https://github.com/arpit73/expense_app",
  "license": "MIT",
  "keywords": [
    "next",
    "react",
    "redux",
    "typescript",
    "styled-components",
    "pleo",
    "challenge"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development next",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export && cd out && now --prod"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "next": "^9.5.4",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.3",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "typescript": "^4.0.3"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,json}": "prettier --write --config ./.prettierrc --ignore-path ./.gitignore"
  }
